<li id="folder_{{ directory.id }}" {% if directory.deleteMark !=0 %} class="entry-content-file-deleted" {% endif %}>
                        <span>
                            {%  if directory.deleteMark != 0 %}
                                <i class="fa fa-remove"></i>
                            {% endif %}
                            <a href="#" name="openFolder" id="{{ directory.id }}">
                                <i class="fa fa-angle-down"></i><i class="fa fa-folder"> </i> {{ directory.folderName }}</a>

                        </span>
    <span>ID: {{ directory.addedByUserId }}</span>
    <span>{{ directory.addTimestamp|date('Y-m-d H:i:s') }}</span>
    <span>
                            {%  if directory.deleteMark != 0 %}
                                <i class="fa fa-ban" id="isRemoved"> </i>
                            {% else %}
                                <i class="fa fa-check-circle-o" id="checkPassed"> </i>
                                <i class="fa fa-exclamation" id="checkError"> </i>
                                <i class="fa fa-user" id="lastChangeInfo"> </i>
                            {% endif %}

                        </span>
    <span>
                            {%  if directory.deleteMark != 0 %}
                                <a href="#" name="restoreFolder" id="{{ directory.id }}"><i class="fa fa-undo"></i></a>
                            {% else %}
                                <!--<a href="#" name="openContents" id="{{ directory.id }}"><i class="fa fa-angle-down"></i><i class="fa fa-folder" id="openContents"></i></a>-->
                                <a href="#" name="addFiles" id="{{ directory.id }}"><i class="fa fa-upload" id=addFiles"> </i></a>
                                <a href="#" name="createCatalogue" id="{{ directory.id }}"><i class="fa fa-plus" id="createCatalogue"> </i></a>
                                <a href="#" name="removeFolder" id="{{ directory.id }}"><i class="fa fa-trash-o" id="removeFolder"> </i></a>
                            {% endif %}
                        </span>
</li>