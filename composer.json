{
    "name": "ennor/lencor",
    "license": "proprietary",
    "type": "project",
    "description": "Lencor archive base",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": "^7.1",
        "components/bootstrap": "^3.3",
        "components/jquery": "^3.2",
        "doctrine/annotations": "^1.6",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/orm": "^2.6",
        "friendsofsymfony/elastica-bundle": "^4.1",
        "friendsofsymfony/user-bundle": "^2.0",
        "glifery/entity-hidden-type-bundle": "^2.1",
        "incenteev/composer-parameter-handler": "^2.1",
        "jms/i18n-routing-bundle": "^2.0",
        "jms/serializer-bundle": "^2.3",
        "monolog/monolog": "^1.23",
        "ninsuo/symfony-collection": "^2.1",
        "php-extended/php-tail": "^2.0",
        "robloach/component-installer": "^0.2.3",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/console": "^3.4",
        "symfony/filesystem": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/form": "^3.4",
        "symfony/ldap": "^3.4",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/serializer": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1.6",
        "symfony/symfony": "3.4.*",
        "symfony/templating": "^3.4",
        "symfony/yaml": "^3.4",
        "twbs/bootstrap": "^3.3",
        "twig/twig": "~2.0",
        "vich/uploader-bundle": "^1.7",
        "yavin/symfony-form-tree": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1",
        "symfony/phpunit-bridge": "^3.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1.12"
        },
        "sort-packages": true,
	"discard-changes": true,
	"component-dir": "web/"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
